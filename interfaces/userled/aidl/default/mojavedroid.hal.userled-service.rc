service mojavedroid.hal.userled-service /vendor/bin/hw/mojavedroid.hal.userled-service
    interface aidl mojavedroid.hal.userled-service.IUserLed/default
    class hal
    user system
    group system

on boot
    chown system system /sys/bus/iio/devices/iio:device0/in_humidityrelative_scale
    chmod 0660 /sys/bus/iio/devices/iio:device0/in_humidityrelative_scale